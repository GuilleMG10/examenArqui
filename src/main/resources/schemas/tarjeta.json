{
    "type": "object",
    "properties": {
      "cardNumber": {
        "type": "string",
        "pattern": "^[0-9]{16}$"
      },
      "expirationDate": {
        "type": "string",
        "pattern": "^(0[1-9]|1[0-2])\/([0-9]{2})$"
      },
      "cvv": {
        "type": "string",
        "pattern": "^[0-9]{3}$"
      }
    },
    "required": ["cardNumber", "expirationDate", "cvv"]
  }
  